{
"manifest_version": 2,
    "name": "dejargon",
    "short_name" : "dejargon",
    "description": "Displays user-provided translations for terms",
    "version": "0.1.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "dejargon.js"
            ],
            "css": [
                "dejargon.css"
            ],

            "run_at": "document_end"
        }
    ],
    "permissions" : ["activeTab", "storage"],
    "options_page" : "options.html",
    "icons" : {
	"16" : "book-16px.png",
	"128" : "book-128px.png"
    }
}
